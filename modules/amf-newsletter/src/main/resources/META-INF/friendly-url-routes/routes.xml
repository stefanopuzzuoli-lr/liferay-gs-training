<?xml version="1.0"?>
 <!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.2.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_2_0.dtd">

 <routes>
     <route>
         <pattern>/-/article-issue/{folderId}-{issueNumber:\d+}</pattern>
         <ignored-parameter name="createDate"/>
         <implicit-parameter name="mvcPath">/view_issue.jsp</implicit-parameter>
         <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
         <implicit-parameter name="p_p_state">normal</implicit-parameter>
         <implicit-parameter name="p_p_mode">view</implicit-parameter>
     </route>
 </routes>